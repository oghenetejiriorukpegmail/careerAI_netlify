# Sample environment variables for CareerAI
# Copy this file to .env and fill in your own values

# AI API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
ROUTER_API_KEY=your_requesty_router_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Google Document AI Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_APPLICATION_CREDENTIALS=google_document_ai.json
GOOGLE_PROJECT_ID=your_google_project_id_here
GOOGLE_PROCESSOR_ID=your_google_processor_id_here

# Supabase Configuration
DATABASE_URL=your_database_url_here
SUPABASE_SERVICE_ROLE_SECRET=your_supabase_service_role_secret_here  
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Vertex AI Configuration (if using)
VERTEX_API_KEY=your_vertex_api_key_here

# Bright Data Configuration (Optional - for enhanced web scraping)
# Sign up at https://brightdata.com to get credentials
# This enables scraping of dynamically loaded job postings and LinkedIn profiles
BRIGHT_DATA_USERNAME=your_bright_data_username
BRIGHT_DATA_PASSWORD=your_bright_data_password
BRIGHT_DATA_ZONE=data_center # Optional, defaults to 'data_center'